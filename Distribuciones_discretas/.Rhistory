hipergeometrica <- function(x, K, N, n, acumulada = FALSE){
if(acumulada == FALSE){
resultado <- phyper(x, K, N-K, n)- phyper(x-1, K, N-K, n)
}else{
resultado <- phyper(x, K, N-K, n)
}
return(resultado)
}
